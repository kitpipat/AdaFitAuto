        "NameInSource": "Pos",
        "CommandTimeout": 5000,
        "SqlCommand": "SELECT HD.FTBchCode,BchL.FTBchName,ISNULL(HD.FTSplCode,'') AS FTSplCode,ISNULL(FTXphRmk,'') AS FTXphRmk,\r\n\tISNULL(Spl_L.FTSplName,'') AS FTSplName,ISNULL(Spl.FTSplTel,'') AS FTSplTel,ISNULL(Spl.FTSplFax,'') AS FTSplFax,\r\n\tHD.FTXphDocNo,FDXphDocDate,CONVERT(VARCHAR(10),FDXphDocDate,103) AS FTChgDate,CONVERT(VARCHAR(10),FDXphDocDate,8) AS FTXphDocTime,\r\n\tHD.FTCreateBy,UsrCre_L.FTUsrName AS FTCreName,\r\n\tUsrCre_L.FTUsrCode AS FTCreCode,\r\n\tISNULL(HD.FTDptCode,'') AS FTDptCode,Dpl_L.FTDptName,\r\n\tISNULL(HD.FTUsrCode,'') AS FTUsrCode,Usr_L.FTUsrName,\t\r\n\tISNULL(FTXphStaDoc,'') AS FTXphStaDoc,\r\n\tISNULL(HD.FTXphApvCode,'') AS FTXphApvCode,\r\n\tISNULL(UsrApvL.FTUsrCode,'') AS FTUsrCodeApp,\r\n\tISNULL(UsrApvL.FTUsrName,'') AS FTUsrNameApp,\r\n\tISNULL(SHD.FTXphCtrName,'') AS FTXphCtrName,\r\n\tISNULL(FTAddVersion,'2') AS FTAddVersion,\r\n\tISNULL(FTAddV2Desc1,'') AS FTAddV2Desc1,\r\n\tISNULL(FTAddV2Desc2,'') AS FTAddV2Desc2,\r\n\tISNULL(SplAddr_L.FTAddRefNo,'') AS FTAddRefNo,\r\n\tISNULL(SplAddr_L.FTAddV1PvnCode,'') AS FTAddV1PvnCode, ISNULL(SplAddr_L.FTAddV1DstCode,'') AS FTAddV1DstCode,\r\n\tISNULL(SplAddr_L.FTAddV1SubDist,'') AS FTAddV1SubDist, \r\n\tISNULL(SplAddr_L.FTAddName,'') AS FTAddName, ISNULL(SplAddr_L.FTAddV1No, '') AS FTAddV1No,\r\n\tISNULL(SplAddr_L.FTAddV1Village, '') AS FTAddV1Village,\r\n\tISNULL(SplAddr_L.FTAddV1Soi, '') AS FTAddV1Soi,\r\n\tISNULL(SplAddr_L.FTAddV1Road, '') AS FTAddV1Road,\r\n\tISNULL(SplAddr_L.FTAddV1PostCode, '') AS FTAddV1PostCode,\r\n\tHD.FTPrdCode,CONVERT(VARCHAR(10),HD.FDXphDueDate,103) AS FTXphDueDate,HD.FTXphCond,\r\n\tSHD.FTXphDstPaid,SHD.FTXphCshOrCrd,SHD.FNXphCrTerm,SHD.FTXphCtrName AS FTXphCtrCode,\r\n\tSHD.FTXphCtrName AS FTCtrName,\r\n\tISNULL(FCXphTotal,0) AS FCXphTotal,\r\n\tISNULL(FCXphGndXC,0) AS FCXphGndXC,\r\n\tISNULL(FCXphGndXN,0) AS FCXphGndXN,\r\n\tISNULL(FCXphGndXX,0) AS FCXphGndXX,\r\n\tISNULL(FCXphGrand,0) AS FCXphGrand,\r\n\tISNULL(FTXphGrandText,'') AS FTXphGrandText,HD.FTXphStaApv\r\n\tFROM TACTPbHD  HD WITH (NOLOCK)  INNER JOIN \r\n     TACTPbHDSpl SHD WITH (NOLOCK) ON HD.FTBchCode = SHD.FTBchCode AND HD.FTXphDocNo = SHD.FTXphDocNo \r\n\t LEFT JOIN  TCNMBranch_L BchL WITH (NOLOCK) ON HD.FTBchCode = BchL.FTBchCode AND BchL.FNLngID =  @pnLanguage LEFT JOIN\r\n\t TCNMSpl Spl WITH (NOLOCK) ON HD.FTSplCode = Spl.FTSplCode LEFT JOIN\r\n\t TCNMSpl_L Spl_L WITH (NOLOCK) ON HD.FTSplCode = Spl_L.FTSplCode AND Spl_L.FNLngID =  @pnLanguage LEFT JOIN\r\n     TCNMSpl_L CtrSpl_L WITH (NOLOCK) ON SHD.FTXphCtrName = CtrSpl_L.FTSplCode AND CtrSpl_L.FNLngID =  @pnLanguage LEFT JOIN\r\n     TCNMUsrDepart_L Dpl_L WITH (NOLOCK) ON  HD.FTDptCode = Dpl_L.FTDptCode AND Dpl_L.FNLngID =  @pnLanguage LEFT JOIN\r\n\t TCNMUser_L Usr_L  WITH (NOLOCK) ON HD.FTUsrCode = Usr_L.FTUsrCode AND Usr_L.FNLngID =  @pnLanguage LEFT JOIN\r\n\t TCNMUser_L UsrCre_L  WITH (NOLOCK) ON HD.FTCreateBy = UsrCre_L.FTUsrCode AND UsrCre_L.FNLngID =  @pnLanguage LEFT JOIN\r\n\t TCNMUser_L UsrApvL  WITH (NOLOCK) ON HD.FTXphApvCode = UsrApvL.FTUsrCode AND UsrApvL.FNLngID =  @pnLanguage LEFT JOIN\r\n\t TCNMSplAddress_L SplAddr_L WITH (NoLock) ON HD.FTSplCode = SplAddr_L.FTSplCode AND SplAddr_L.FTAddGrpType ='1' AND SplAddr_L.FNLngID =  @pnLanguage\r\nWHERE HD.FTXphDocNo = @ptDocNo AND HD.FTBchCode = @ptDocBch\r\n\r\n",
